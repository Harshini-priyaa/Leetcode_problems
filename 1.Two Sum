class Solution {
    public int[] twoSum(int[] nums, int target) {
        //int[] arr;
        for(int i = 0; i < nums.length -1; i+=1){
            for(int j = i+1; j < nums.length; j+=1){
                if(nums[i] + nums[j] == target){
                    return new int[]{i ,j};
                }
            }
        }
        return new int[]{};
    }
}
